<?xml version="1.0"?>
<!-- -->
<launch>

  <arg name="filename1" default="2017-09-06-19-34-51"/>
  <arg name="filename2" default="tibi2"/>
  <arg name="delay"     default="3.0"/>
  <arg name="path"     default="$(env HOME)/rosbags/"/>
  <arg name="rate"     default="1"/>

  <param name="use_sim_time" value="false"/>

  <node pkg="rosbag"
        type="play"
        name="rosbag1"
        args="$(arg path)/$(arg filename1).bag -r $(arg rate)"
        output="screen">
  </node>
  
<!--  <node pkg="rosbag"
        type="play"
        name="rosbag2"
        args="$(arg path)/$(arg filename2).bag -r $(arg rate) -d $(arg delay)"
        output="screen">
  </node>-->

</launch>
